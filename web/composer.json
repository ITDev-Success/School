{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "algolia/algoliasearch-client-php": "*",
        "aws/aws-sdk-php-laravel": "dev-master",
        "barryvdh/laravel-dompdf": "v3.1.1",
        "filament/tables": "^3.2",
        "guzzlehttp/guzzle": "^7.8",
        "jantinnerezo/livewire-alert": "^3.0-stable",
        "kreait/laravel-firebase": "^6.1",
        "laracasts/presenter": "dev-master",
        "laravel-notification-channels/fcm": "^5.0",
        "laravel/framework": "^12.0",
        "laravel/helpers": "*",
        "laravel/legacy-factories": "1.x-dev",
        "laravel/sanctum": "^4.2.0",
        "laravel/scout": "^10.20.0",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6",
        "league/csv": "^9.11",
        "league/flysystem-aws-s3-v3": "^3.24.0",
        "livewire/livewire": "^3.4",
        "maatwebsite/excel": "3.1.x-dev",
        "nckg/laravel-impersonate": "4.0.1",
        "predis/predis": "v3.0.0-alpha1",
        "pusher/pusher-php-server": "7.2",
        "santigarcor/laratrust": "8.x-dev",
        "spatie/laravel-activitylog": "4.10.2",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-medialibrary": "^11.17.0",
        "stevebauman/purify": "v6.3.1",
        "symfony/expression-language": "*",
        "symfony/http-client": "^7.2",
        "symfony/mailgun-mailer": "^7.2",
        "symfony/postmark-mailer": "^7.2",
        "symfony/process": "^7.0",
        "titasgailius/search-relations": "dev-master",
        "twilio/sdk": "dev-main"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.6",
        "beyondcode/laravel-dump-server": "^2.1.0",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "spatie/laravel-web-tinker": "^1.9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "config/school-plus.php",
            "app/Helpers/SiteHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run production"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
